{
	"name": "saturated-dark-modern",
	"displayName": "Saturated Dark Modern",
	"description": "Saturated version of the default Dark Modern theme",
	"version": "1.0.8",
	"publisher": "chneau",
	"license": "MIT",
	"scripts": {
		"start": "bun run --cwd code start",
		"build": "bun run --cwd code build",
		"upgrade": "bun run --cwd code upgrade",
		"publish": "bun run build && vsce package && vsce publish",
		"all": "bun run --cwd code all"
	},
	"galleryBanner": {
		"color": "#1E1E1E",
		"theme": "dark"
	},
	"keywords": ["dark"],
	"repository": {
		"type": "git",
		"url": "https://github.com/chneau/vscode-saturated-dark-modern"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": ["Themes"],
	"contributes": {
		"themes": [
			{
				"label": "Saturated Dark Modern",
				"uiTheme": "vs-dark",
				"path": "./themes/Saturated Dark Modern-color-theme.json"
			}
		]
	}
}
